T].[FirstName] = [S].[FirstName],&#xA;[T].[LastName] = [S].[LastName],&#xA;[T].[Company] = [S].[Company],&#xA;[T].[Email] = [S].[Email],&#xA;[T].[Telephone] = [S].[Telephone],&#xA;[T].[Credit] = [S].[Credit],&#xA;[T].[Activated] = [S].[Activated],&#xA;[T].[Description] = [S].[Description],&#xA;[T].[Website] = [S].[Website],&#xA;[T].[Created] = [S].[Created],&#xA;[T].[ValidTo] = NULL,&#xA;[T].[Active] = 1&#xA;&#xA;WHEN NOT MATCHED THEN&#xA;INSERT (&#xA;[ID],[FirstName],[LastName],[Company],[Email],[Telephone],[Credit],[Activated],[Description],[Website],[Created],[ValidFrom],[ValidTo],[Active])&#xA;VALUES (&#xA;[S].[ID],[S].[FirstName],[S].[LastName],[S].[Company],[S].[Email],[S].[Telephone],[S].[Credit],[S].[Activated],[S].[Description],[S].[Website],[S].[Created],GETDATE(),NULL,1);&#xA;&#xA;/************************************************************ STEP 3 ************************************************************/&#xA;-- Records which have not been loaded to stage -&gt; Set as not Active with corresponding ValidTo date&#xA;UPDATE T &#xA;SET [T].[ValidTo] = GETDATE(), [T].[Active] = 0&#xA;FROM [myDB].[cyrkl].[Customer] AS T&#xA;LEFT JOIN #tmp_Customer AS S&#xA;ON [T].[ID] = [S].[ID]&#xA;WHERE [S].[ID] IS NULL&#xA;AND [T].[Active] = 1;&#xA;&#xA;COMMIT;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
